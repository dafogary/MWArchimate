{
    "name": "MWArchimate",
    "author": "Gary Foster - DAFO Creative Ltd",
    "descriptionmsg": "MWArchimate-desc",
    "url": "https://github.com/dafogary/MWArchimate",
    "version": "0.1.4 Alpha",
    "license-name": "AGPL-3.0-or-later",
    "type": "parserhook",
    "MessagesDirs": {
